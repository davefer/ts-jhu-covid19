{
  "root": true,
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-redeclare": "off",
    "no-unused-vars": "off"
  }
}
